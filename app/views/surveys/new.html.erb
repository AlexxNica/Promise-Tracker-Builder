<%= render 'survey_builder' %>

<script type="text/javascript">
  PT.survey = new PT.SurveyModel();
  PT.selectedInput = ko.observable();
  ko.applyBindings(PT.survey);
  $("#new-survey-modal").modal();
  $("#tool-palette .tool-button").on("click", PT.survey.addInput);
  $("#launch-survey").on("click", PT.launchSurvey);
</script>

